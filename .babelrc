{
  "presets": ["es2015"],
  "env": {
    "CASSOWARYJS": {
      "plugins": [
        "transform-inline-environment-variables",
        ["babel-plugin-module-alias", [
          { "src": "npm:cassowary", "expose": "constraint-solver" }
        ]]
      ]
    },
    "KIWI": {
      "plugins": [
        "transform-inline-environment-variables",
        ["babel-plugin-module-alias", [
          { "src": "npm:kiwi-solver", "expose": "constraint-solver" }
        ]]
      ]
    }
  }
}
